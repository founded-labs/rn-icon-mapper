{
  "name": "rn-icon-mapper",
  "version": "0.0.0-alpha.1",
  "description": "A lightweight utility for mapping icons names between **SF Symbols**, **Material Icons**, and **Material Community Icons**.",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "pnpm gen && tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist node_modules",
    "pub:beta": "pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm publish --no-git-checks --access public --tag next",
    "pub:release": "pnpm publish --access public",
    "gen:sf-symbol-mapping": "tsx scripts/generate-sf-symbol-mapping.ts",
    "gen:md-icon-table": "tsx scripts/generate-md-icon-table.ts",
    "gen": "pnpm gen:sf-symbol-mapping && pnpm gen:md-icon-table"
  },
  "devDependencies": {
    "@tsconfig/react-native": "^1.0.1",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "tsup": "^8.1.0",
    "tsx": "^4.20.3"
  }
}
